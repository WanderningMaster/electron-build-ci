{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "build": "cargo-cp-artifact -nc index.node -- cargo build  --message-format=json-render-diagnostics",
